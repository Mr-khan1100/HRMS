{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@appHooks/*": ["src/appHooks/*"],
      "@navigation/*": ["src/navigation/*"],
      "@assets/*": ["./src/assets/*"],
      "@screens/*": ["./src/screens/*"],
      "@redux/*": ["./src/redux/*"],
      "@constants/*": ["./src/constants/*"],
      "@sharedComponents/*": ["./src/sharedComponents/*"],
      "@styles/*": ["./src/styles/*"],
    }
  }
}
